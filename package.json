{
	"name": "hhc",
	"displayName": "编程日历",
	"description": "《了不起的程序员2021》编程日历配套的 VS Code 日历扩展",
	"icon": "calendar.png",
	"repository": {
		"url": "https://github.com/Sneezry/vscode-hhc.git"
	},
	"publisher": "sneezry",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hhc.today",
				"title": "编程日历：今日"
			},
			{
				"command": "hhc.tomorrow",
				"title": "编程日历：明日"
			},
			{
				"command": "hhc.yesterday",
				"title": "编程日历：昨日"
			},
			{
				"command": "hhc.week",
				"title": "编程日历：本周"
			},
			{
				"command": "hhc.lastweek",
				"title": "编程日历：上周"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"solarlunar": "^2.0.7"
	}
}
